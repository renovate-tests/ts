{
  "name": "@9renpoto/ts",
  "description": "study frontend",
  "version": "0.0.1",
  "author": "9renpoto",
  "dependencies": {},
  "devDependencies": {
    "@9renpoto/eslint-config": "^1.4.0",
    "@9renpoto/tslint-config": "^2.0.0",
    "@types/mocha": "^2.2.33",
    "@types/power-assert": "^1.4.29",
    "codecov": "^1.0.1",
    "eslint": "^3.14.1",
    "intelli-espower-loader": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.0",
    "power-assert": "^1.4.2",
    "remap-istanbul": "^0.9.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.1",
    "rollup-plugin-typescript": "^0.8.1",
    "tslint": "^4.4.2",
    "typescript": "^2.0.10",
    "typescript-eslint-parser": "^1.0.0"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "codecov",
    "example",
    "typescript"
  ],
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/javascript.git"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf dist coverage",
    "codecov": "npm run cover && codecov -f coverage/coverage-remapped.json",
    "cover": "npm-run-all -p cover:*",
    "cover:html": "cd coverage && remap-istanbul -i coverage.json -o html-report -t html",
    "cover:json": "cd coverage && cat coverage.json | remap-istanbul -o coverage-remapped.json",
    "lint": "npm-run-all -p lint:*",
    "lint:es": "eslint . --ext=ts,tsx",
    "lint:ts": "tslint \"src/*.ts\" \"test/*.ts\"",
    "posttest": "npm run cover",
    "pretest:mocha": "npm run clean && tsc",
    "test": "npm-run-all -p test:*",
    "test:build": "npm run build",
    "test:lint": "npm run lint",
    "test:mocha": "istanbul cover _mocha -- dist/test/*.test.js"
  }
}
